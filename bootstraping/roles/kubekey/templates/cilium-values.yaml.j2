kubeProxyReplacement: "true"
k8sServicePort: "{{ api_server_port }}"
k8sServiceHost: "{{ kube_vip_address }}"
k8sClientRateLimit:
  qps: 20
  burst: 100
l2announcements:
  enabled: "true"
  leaseDuration: 3s
  leaseRenewDeadline: 1s
  leaseRetryPeriod: 200ms
hubble:
  enabled: "true"
  relay:
    enabled: "true"
  ui:
    enabled: "true"
gatewayAPI:
  enabled: "true"
#enableIPv4BIGTCP: "true"
bpf:
  masquerade: "true"
  hostLegacyRouting: "false"
externalIPs:
  enabled: "true"
rollOutCiliumPods: "true"
endpointRoutes:
  enabled: "true"
ipam:
  operator:
    clusterPoolIPv4PodCIDRList: ["10.233.64.0/18"] # 10.0.0.0/8
ipv4NativeRoutingCIDR: "10.233.64.0/18"
# routingMode: "native"
# autoDirectNodeRoutes: true
hostPort:
  enabled: "false"
nodePort:
  enabled: "false"
